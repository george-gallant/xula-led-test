#	xula2

NET clk_i loc=A9;		# 12 MHz clock to FPGA.

NET "clk_i" TNM_NET = "clk_i";
TIMESPEC "TSclk_i" = PERIOD "clk_i" 12 MHz HIGH 50%;
NET "clk_i" CLOCK_DEDICATED_ROUTE = FALSE;

