NET s_o<2> loc=C1;		 # chan_io<28>
NET s_o<3> loc=F2;		 # chan_io<25>
NET s_o<4> loc=B16;		 # chan_io<13>
NET s_o<5> loc=E1;		 # chan_io<26>
NET s_o<6> loc=B15;		 # chan_io<14>
NET s_o<7> loc=F16;		 # chan_io<10>
